{
  "$id": "https://raw.githubusercontent.com/Monster-Train-2-Modding-Group/Trainworks-Reloaded/refs/heads/main/schemas/definitions/parse_term.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "minLength": 1
    },
    "description": {
      "type": "string"
    },
    "group": {
      "type": "string",
      "minLength": 1
    },
    "speaker_descriptions": {
      "type": "string"
    },
    "english": {
      "type": "string",
      "minLength": 1
    },
    "french": {
      "type": "string",
      "minLength": 1
    },
    "german": {
      "type": "string"
    },
    "russian": {
      "type": "string"
    },
    "portuguese": {
      "type": "string"
    },
    "chinese": {
      "type": "string"
    },
    "spanish": {
      "type": "string"
    },
    "chinese_traditional": {
      "type": "string"
    },
    "korean": {
      "type": "string"
    },
    "japanese": {
      "type": "string"
    },
    "other_languages": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["language", "translation"],
        "properties": {
          "language": {
            "type": "string"
          },
          "translation": {
            "type": "string"
          }
        }
      }
    }
  }
}